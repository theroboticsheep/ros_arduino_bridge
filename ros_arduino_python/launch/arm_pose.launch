<launch>
   <arg name="animation" default="~proto-demo" />
   <arg name="sequence" default="sequence1" />
   <arg name="pose_name" default="~home" />
   <rosparam param="animation" subst_value="True">$(arg animation)</rosparam>
   <rosparam param="sequence" subst_value="True">$(arg sequence)</rosparam>
   <rosparam param="pose_name" subst_value="True">$(arg pose_name)</rosparam>
   <node name="arm_pose" pkg="ros_arduino_python" type="arm_pose_node.py" output="screen">
      <rosparam file="$(find ros_arduino_python)/config/arm_poses.yaml" command="load" />
   </node>
</launch>
